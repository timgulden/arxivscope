// custom.js
document.addEventListener('DOMContentLoaded', function() {
    const floatingFilters = document.querySelectorAll('.ag-floating-filter-container');
    floatingFilters.forEach(function(filter) {
        if (!filter.querySelector('input')) {
            filter.style.display = 'none';
            filter.style.height = '0';
            filter.style.padding = '0';
            filter.style.margin = '0';
        }
    });
});
